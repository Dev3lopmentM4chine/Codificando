algoritmo "Calculo V1"
var
   //Declarando as variaveis
   n1, n2, Soma: Inteiro
inicio
      //Pede um dois valores e depois faz uma soma
      Escreva("Digite o primeiro número: ")
      Leia(n1)
      Escreva("Digite o segundo número: ")
      Leia(n2)
      Soma <- n1 + n2
      Escreva("A soma é ", Soma)//Resultado
fimalgoritmo